{
  "name": "prompt-hub",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "validate:prompts": "node scripts/validate-prompts.js",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .ts,.vue",
    "format": "prettier --write .",
    "test": "vitest run",
    "prepare": "husky install"
  },
  "dependencies": {
    "vue": "^3.4.21",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "vue-tsc": "^2.0.6",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.20.1",
    "@typescript-eslint/parser": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "husky": "^9.1.0",
    "lint-staged": "^15.2.2",
    "vitest": "^1.6.1"
  },
  "lint-staged": {
    "*.{ts,vue,js,json}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}

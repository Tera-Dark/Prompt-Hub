{
  "name": "prompt-hub",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "validate:prompts": "node scripts/validate-prompts.js",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .ts,.vue",
    "format": "prettier --write .",
    "test": "vitest run",
    "prepare": "husky install"
  },
  "dependencies": {
    "@octokit/rest": "^22.0.1",
    "vue": "^3.4.21",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "13.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.20.1",
    "husky": "^9.1.0",
    "jsdom": "^27.2.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.3.3",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "vitest": "^1.6.1",
    "vue-tsc": "^2.0.6"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "eslint --fix"
    ],
    "*.{ts,vue,js,json}": [
      "prettier --write"
    ]
  }
}
